  "betterScripts": {
    "start-dev": {
      "command": "webpack-dev-server --progress --hot --colors --history-api-fallback --display-error-details",
      "env": {
        "API_ENDPOINT": "https://0eyk6qucwj.execute-api.us-east-1.amazonaws.com/dev/resource/graphql",
        "BABEL_ENV": "development",
        "DEVELOPMENT": true,
        "DEVTOOLS": true
      }
    },
    "build-dev": {
      "command": "webpack -p",
      "env": {
        "API_ENDPOINT": "https://0eyk6qucwj.execute-api.us-east-1.amazonaws.com/dev/resource/graphql",
        "BABEL_ENV": "production",
        "NODE_ENV": "production"
